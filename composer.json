{
    "name": "bhuvidya/laravel-query-logger",
    "description": "A tool to log all database queries in a Laravel application. Bug kudos to `overtrue`, on whose work this package is originally based (overtrue/laravel-query-logger).",
    "authors": [
        {
            "name": "bhu Boue vidya",
            "email": "bhuvidya@gmail.com",
            "role": "developer"
        }
    ],
    "keywords": [
        "laravel",
        "database",
        "query",
        "log"
    ],
    "homepage": "https://github.com/bhuvidya/laravel-query-logger",
    "license": "MIT",
    "require": {
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "php": ">=7.2.0"
    },
    "autoload": {
        "psr-4": {
            "BhuVidya\\QueryLogger\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "source": "https://github.com/bhuvidya/laravel-query-logger",
        "issues": "https://github.com/bhuvidya/laravel-query-logger/issues"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BhuVidya\\QueryLogger\\QueryLoggerServiceProvider"
            ]
        }
    }
}
